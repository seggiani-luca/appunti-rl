#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x56b73465e500 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x56b7346a7b30_0 .var "t_x", 0 0;
v0x56b7346a7bd0_0 .net "t_z", 0 0, v0x56b7346a7a10_0;  1 drivers
S_0x56b73465e690 .scope module, "i_gen" "posedge_impulse_generator" 2 5, 3 3 0, S_0x56b73465e500;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "x";
    .port_info 1 /OUTPUT 1 "z";
v0x56b734697d00_0 .net "x", 0 0, v0x56b7346a7b30_0;  1 drivers
v0x56b7346a7a10_0 .var "z", 0 0;
E_0x56b7346984f0 .event posedge, v0x56b734697d00_0;
    .scope S_0x56b73465e690;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56b7346a7a10_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x56b73465e690;
T_1 ;
    %wait E_0x56b7346984f0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56b7346a7a10_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56b7346a7a10_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x56b73465e500;
T_2 ;
    %vpi_call 2 10 "$dumpfile", "posedge_impulse_generator_waveform.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56b7346a7b30_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56b7346a7b30_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56b7346a7b30_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tests/posedge_impulse_generator_bench.v";
    "impulse_generators/posedge_impulse_generator.v";
